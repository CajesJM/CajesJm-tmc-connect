{
  "expo": {
    "name": "TMC Connect",
    "slug": "TMC-Connect",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Logo/TMC_Connect.png",
    "scheme": "tmccampushub",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/Logo/TMC_Connect.png",
        "backgroundColor": "#E6F4FE"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.cajes.tmccampushub"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/Logo/TMC_Connect.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/Logo/TMC_Connect.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "d9a509a5-fe90-4c5b-b337-01bdab365205"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/d9a509a5-fe90-4c5b-b337-01bdab365205"
    }
  }
}
